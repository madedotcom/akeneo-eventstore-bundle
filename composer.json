{
    "name": "madedotcom/eventstore-bundle",
    "type": "symfony-bundle",
    "description": "Madedotcom EventStore Bundle",
    "keywords": ["Madedotcom", "EventStore"],
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "akeneo/pim-community-dev": "^4.0.0",
        "symfony/framework-bundle": "4.4.1",
        "symfony/console": "4.3.3",
        "doctrine/persistence": "1.2.0",
        "justinrainbow/json-schema": "^5.2.0"
    },
    "require-dev": {
        "phpspec/phpspec": "6.0.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-community-dev.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Madedotcom\\Bundle\\EventStoreBundle\\": ""
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
