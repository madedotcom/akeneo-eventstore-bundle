{
    "name": "madedotcom/eventstore-bundle",
    "type": "symfony-bundle",
    "description": "Madedotcom EventStore Bundle",
    "keywords": ["Madedotcom", "EventStore"],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "akeneo/pim-community-dev": "^2.0",
        "ramsey/uuid": "*",
        "hadesarchitect/json-schema-bundle": "*"
    },
    "require-dev": {
        "phpspec/phpspec": "2.1.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-community-dev.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Madedotcom\\Bundle\\EventStoreBundle\\": ""
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
